              {
                  String output = getMethodOutput("main");
                  String expect = "0.67";
                  boolean passed = output.contains(expect);
                  passed = getResults(expect, output, "Expected output from main", passed);
                  assertTrue(passed);
              }
          }
        </tests>
      </program>
    </activity>

